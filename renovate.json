{
  "extends": ["config:base"],
  "assignees": ["mhutter"],
  "regexManagers": [{
    "fileMatch": ["\\.ya?ml$"],
    "matchStrings": [
      "depName=(?<depName>.*?)( versioning=(?<versioning>.*?))?\\s.+_version: \"?(?<currentValue>.*?)\"?\\s"
    ],
    "datasourceTemplate": "github-releases",
    "versioningTemplate": "{{#if versioning}}{{{versioning}}}{{else}}semver{{/if}}"
  }]
}
