{
  "extends": ["config:base"],
  "assignees": ["mhutter"],
  "regexManagers": [{
    "fileMatch": ["\\.ya?ml$"],
    "matchStrings": [
      "depName=(?<depName>.*?)?\\s.+_version: \"?(?<currentValue>.*?)\"?\\s"
    ],
    "datasourceTemplate": "github-releases"
  }],
  "packageRules": [{
    "matchPackageNames": ["minio/minio"],
    "versioning": "regex:^RELEASE\\.(?<major>\\d+)-(?<minor>\\d+)-(?<patch>\\d+)T(?<build>.+)Z$"
  }]
}
